{
  "env": {},
  "permissions": {
    "allow": [],
    "deny": []
  },
  "hooks": {
    "Notification": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "ffplay -autoexit -nodisp /usr/share/sounds/freedesktop/stereo/message.oga"
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "ffplay -autoexit -nodisp /usr/share/sounds/freedesktop/stereo/bell.oga"
          }
        ]
      }
    ]
  },
  "enabledPlugins": {
    "typescript-lsp@claude-plugins-official": true,
    "gopls-lsp@claude-plugins-official": true
  },
  "model": "sonnet"
}
